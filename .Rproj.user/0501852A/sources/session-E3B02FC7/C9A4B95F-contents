require(palmerpenguins)

penguins <- penguins

##Q1

rm(list = ls())

sse_mean = function(x)
{
  n <- length(x)
  x <- x[!is.na(x)]
  
  sse <- sqrt((var(x))) / sqrt(n)
  
  return(sse)  
}

sse_mean(penguins$body_mass_g)
sse_mean(mtcars$mpg)

